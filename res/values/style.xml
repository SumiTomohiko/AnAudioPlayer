<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE resources>
<resources
    xmlns:android="http://schemas.android.com/apk/res/android">
    <style name="Text">
        <item name="android:textSize">36sp</item>
        <item name="android:textColor">#000000</item>
    </style>
    <style name="TimeText" parent="Text">
        <item name="android:typeface">monospace</item>
    </style>
    <style name="Button">
        <item name="android:background">@drawable/button</item>
    </style>
    <style name="NavRightButton" parent="Button">
        <item name="android:src">@drawable/nav_right</item>
    </style>
    <style name="NavLeftButton" parent="Button">
        <item name="android:src">@drawable/nav_left</item>
    </style>
    <style name="DisabledButton" parent="Button">
        <item name="android:clickable">false</item>
        <item name="android:src">@drawable/ic_blank</item>
    </style>
    <style name="List">
        <item name="android:background">@color/background</item>
        <item name="android:listSelector">@drawable/row</item>
    </style>
</resources>
